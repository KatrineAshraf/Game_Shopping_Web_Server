<html>
<head> 
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Cinzel&display=swap" rel="stylesheet">
    <link rel='stylesheet' href='stylesheets/tables.css' />


</head>
<body>
    <table><tr>     <th>Order ID</th> <th>Customer</th><th colspan="2">Product</th>  <th>Price</th>   <th>Ordering Time</th> <th>Status</th>  </tr> 
        <%if (data.length != 0){ 
        for (var i = 0; i < data.length; i++){ %>
        <tr>  <td> <%= data[i].id %> </td> <td><%= data[i].c_name %></td> <td colspan="2"> <img src="<%=data[i].image%>"><%=data[i].p_name %> </td>  <td> <%=data[i].price %> </td> <td> <%=data[i].ordered_at%> </td> 
        <td><% if(data[i].state == 0){%> Pending <%
        }else if (data[i].state == 1){%> Successful <% } else {%> Error <%} %></td> </tr>
       <% }
        }  
        else{ 
        %>
        <td colspan="7">No Data Found</td>  </tr>
        <% } %>
        </table>
</body>
</html>